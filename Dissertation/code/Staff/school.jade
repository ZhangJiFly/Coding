      .row
        .col-md-6
          .panel.panel-primary
            .panel-heading
              = school + " Degrees"
            .panel-body 
              table#degrees.display(cellpadding='10', cellspacing='10', border='1')
        .col-md-6
          .panel.panel-primary
            .panel-heading 
              = school + " Course Lists"
            .panel-body 
              table#courseLists.display(cellpadding='10', cellspacing='10', border='1')
